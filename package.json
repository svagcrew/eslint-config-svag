{
  "name": "svag-lint",
  "version": "0.0.24",
  "homepage": "https://github.com/svagcrew/svag-lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svagcrew/svag-lint.git"
  },
  "bugs": {
    "url": "https://github.com/svagcrew/svag-lint/issues"
  },
  "author": {
    "name": "Sergei Dmitriev",
    "url": "https://sergei.svag.group"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./configs/**/*",
    "./dist/**/*"
  ],
  "bin": {
    "svag-lint": "./dist/index.js",
    "svag-lint!": "./src/index.ts"
  },
  "scripts": {
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache .",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "types": "tsc --noEmit",
    "build": "rimraf ./dist && tsc --build"
  },
  "dependencies": {
    "@eslint/js": "^9.0.0",
    "dedent": "^1.5.3",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lodash": "^4.17.21",
    "svag-cli-utils": "^0.0.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^7.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "rimraf": "^5.0.5",
    "svag-prettier": "~0.0.4",
    "svag-ts": "^0.0.16"
  },
  "libalibe": {
    "depsVersionAccuracyNotMatter": [
      "svag-cli-utils",
      "svag-prettier",
      "svag-ts"
    ]
  }
}
